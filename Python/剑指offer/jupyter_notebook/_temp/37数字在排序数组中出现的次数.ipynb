    "# 作弊了：有待用书上的方法写\n",
    "# -*- coding:utf-8 -*-\n",
    "class Solution:\n",
    "    def GetNumberOfK(self, data, k):\n",
    "        # write code here\n",
    "        a=0\n",
    "        for i in range(len(data)):\n",
    "            if data[i]==k:\n",
    "                a+=1\n",
    "        return a"
